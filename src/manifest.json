{
  "name": "Bookmark Finder",
  "description": "ブックマークを絞り込み検索する拡張機能です。",
  "version": "0.5.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup/index.html"
  },
  "content_scripts": [],
  "options_page": "options.html",
  "permissions": [
    "<all_urls>",
    "storage",
    "bookmarks",
    "tabs"
  ]
}