{
  "name": "Bookmark Finder",
  "description": "ブックマークを絞り込み検索する拡張機能です。",
  "version": "0.1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup/index.html"
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["jquery-2.2.0.min.js", "ChEx.js", "content_script.js"]
  }],
  "options_page": "options.html",
  "permissions": [
    "<all_urls>",
    "storage"
  ]
}